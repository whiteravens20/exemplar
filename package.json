{
  "name": "discord-ai-bot",
  "version": "2.1.0",
  "description": "Discord Bot with n8n AI Assistant integration",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "bash scripts/test-bot.sh",
    "test:unit": "node tests/database.test.js && node tests/admin-stats-types.test.js && node tests/final-result.test.js",
    "test:all": "npm run test && npm run test:unit",
    "test-config": "node test-config.js",
    "deploy-commands": "node src/deploy-commands.js",
    "release-package": "bash scripts/create-release-package.sh",
    "lint": "eslint src --fix",
    "migrate:up": "node scripts/migrate.js up",
    "migrate:down": "node scripts/migrate.js down",
    "db:seed": "bash scripts/seed-test-data.sh"
  },
  "keywords": [
    "discord",
    "bot",
    "ai",
    "n8n",
    "moderation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.5",
    "discord.js": "^14.25.1",
    "dotenv": "^17.3.1",
    "express": "^4.22.1",
    "pg": "^8.18.0",
    "undici": "^7.21.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "nodemon": "^3.1.11"
  },
  "overrides": {
    "undici": "^7.21.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=11.0.0"
  }
}
