{
  "name": "discord-ai-bot",
  "version": "3.0.0",
  "description": "Discord Bot with n8n AI Assistant integration",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts --exec \"npx tsc && node dist/index.js\"",
    "watch": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:all": "vitest run",
    "test:watch": "vitest",
    "deploy-commands": "node dist/deploy-commands.js",
    "release-package": "bash scripts/create-release-package.sh",
    "lint": "eslint src --fix",
    "migrate:up": "npx tsx scripts/migrate.ts up",
    "migrate:down": "npx tsx scripts/migrate.ts down",
    "db:seed": "bash scripts/seed-test-data.sh"
  },
  "keywords": [
    "discord",
    "bot",
    "ai",
    "n8n",
    "moderation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.5",
    "discord.js": "^14.25.1",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "pg": "^8.18.0",
    "undici": "^7.21.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/express": "^5.0.6",
    "@types/node": "^25.3.3",
    "@types/pg": "^8.18.0",
    "eslint": "^10.0.1",
    "globals": "^17.3.0",
    "nodemon": "^3.1.14",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "undici": "^7.21.0",
    "minimatch": "^10.2.4"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=11.0.0"
  }
}
